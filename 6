import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.print("Введите перемменнную a: ");
        int a=scan.nextInt();
        System.out.print("Введите переменную b: ");
        int b=scan.nextInt();
        if (a == b)//уменьшаем время компиляции программы тем, что убираем лишние действоя в двнной ситуации
        {
            System.out.println("переменная a="+ a);
            System.out.println("переменная b="+ b);
        } else {
            b = b - a; //b=9, a=5; b=9-5=4
            a = a + b; //a=5+4=9
            b = a - b; //b=9-4=5
            System.out.println("переменная a=" + a);
            System.out.println("переменная b=" + b);
        }
    }
}
